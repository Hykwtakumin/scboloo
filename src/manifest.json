{
  "name": "Scboloo",
  "description": "easy way to post webpage to scrapbox.io",
  "author": "pastak",
  "version": "0.2.1",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "notifications",
    "tabs",
    "storage",
    "cookies"
  ],
  "applications": {
    "gecko": {
      "id": "scboloo@pastak.net",
      "strict_min_version": "52.0"
    }
  },
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "option/option.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_popup": "/popup/popup.html"
  },
  "manifest_version": 2
}
