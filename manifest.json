{
  "name": "YAPC Hokkaido Sample",
  "version": "0.0.1",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "notifications",
    "tabs"
  ],
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "browser_action": {},
  "content_scripts": [
    {
      "matches": ["https://scrapbox.io/*"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}
